<Window x:Class="pr18_elik.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:pr18_elik"
        mc:Ignorable="d"
        Title="Управление авиалиниями" Height="450" Width="700">
    <Grid>
        <Grid Height="60" VerticalAlignment="Top" Background="#FF0C73FE">
            <Image Width="45" Height="44" VerticalAlignment="Center" HorizontalAlignment="Left" Margin="8,0,0,0" Source="/image/i.png"/>
            <Label VerticalAlignment="Top" HorizontalAlignment="Left" Content="Управление авиалиниями" Foreground="White" Margin="65,12,0,0" FontSize="16"/>
            <Button VerticalAlignment="Center" HorizontalAlignment="Left" Width="100" Height="24" Content="Выйти" Margin="590,0,0,0" BorderBrush="White" Foreground="#FF262222" FontWeight="Bold" FontSize="16" Background="White"/>
        </Grid>
        <Frame x:Name="frame" NavigationUIVisibility="Hidden" Margin="0,60,-0.4,0" />
    </Grid>
</Window>
